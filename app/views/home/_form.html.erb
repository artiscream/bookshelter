<%= form_for([@user,@user.feedbacks.build]) do |f| %>
<p>
<%= f.label :subject %><br>
<%= f.text_area :subject %>
</p>

<p>
<%= f.label :message %><br>
<%= f.text_area :message %>
</p>

<%= hidden_field_tag 'feedbacks[user_id]', nil, :value => '#{current_user.id}' %>

<p>
<%= f.submit %>
</p>

<% end %>